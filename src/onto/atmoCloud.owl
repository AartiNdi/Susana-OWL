    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmo.owl#hasMeteorologicalPhenomena -->

    <owl:ObjectProperty rdf:about="&atmo;#hasMeteorologicalPhenomena"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#hasCloud -->

    <owl:ObjectProperty rdf:about="#hasCloud">
        <rdfs:range rdf:resource="#Cloud"/>
    </owl:ObjectProperty>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/chem.owl#hasSubstance -->

    <owl:ObjectProperty rdf:about="&chem;#hasSubstance"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/phys.owl#hasPhysicalPhenomena -->

    <owl:ObjectProperty rdf:about="&phys;#hasPhysicalPhenomena"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/phys.owl#hasPhysicalProcess -->

    <owl:ObjectProperty rdf:about="&phys;#hasPhysicalProcess"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/phys.owl#hasPhysicalProperty -->

    <owl:ObjectProperty rdf:about="&phys;#hasPhysicalProperty"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/space.owl#upperPartOf -->

    <owl:ObjectProperty rdf:about="&space;#upperPartOf"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://sweet.jpl.nasa.gov/2.0/astroPlanet.owl#PlanetaryBoundary -->

    <owl:Class rdf:about="&planet;#PlanetaryBoundary"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmo.owl#MeteorologicalPhenomena -->

    <owl:Class rdf:about="&atmo;#MeteorologicalPhenomena"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoBoundary.owl#CappingInversion -->

    <owl:Class rdf:about="&bound;#CappingInversion"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#AdiabaticCondensationLevel -->

    <owl:Class rdf:about="#AdiabaticCondensationLevel">
        <rdfs:subClassOf rdf:resource="#CondensationLevel"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#Altostratus -->

    <owl:Class rdf:about="#Altostratus">
        <rdfs:subClassOf rdf:resource="#Stratiform"/>
        <rdfs:comment xml:lang="en"
            >A principal cloud type (cloud genus) in the form of a gray or bluish (never white) sheet or layer of striated, fibrous, or uniform appearance.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#AnvilCloud -->

    <owl:Class rdf:about="#AnvilCloud">
        <rdfs:label>Anvil Cloud</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Cloud"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&space;#upperPartOf"/>
                <owl:allValuesFrom rdf:resource="#Cumulonimbus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>a type of cloud</rdfs:comment>
        <rdfs:comment xml:lang="en"
            >The anvil-shaped cloud that comprises the upper portion of mature cumulonimbus clouds</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#BannerCloud -->

    <owl:Class rdf:about="#BannerCloud">
        <rdfs:subClassOf rdf:resource="#Cloud"/>
        <rdfs:comment xml:lang="en"
            >A cloud plume often observed to extend downwind from isolated, sharp, often pyramid-shaped mountain peaks, even on otherwise cloud-free days.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#BaroclinicLeaf -->

    <owl:Class rdf:about="#BaroclinicLeaf">
        <rdfs:subClassOf rdf:resource="#Cloud"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess"/>
                <owl:allValuesFrom rdf:resource="&press;#Cyclogenesis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >A synoptic-scale cloud pattern frequently observed in satellite imagery just prior to the onset of cyclogenesis</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#BergeronFindeisenProcess -->

    <owl:Class rdf:about="#BergeronFindeisenProcess">
        <rdfs:subClassOf rdf:resource="&solid;#Coalescence"/>
        <rdfs:comment xml:lang="en"
            >A theoretical explanation of the process by which precipitation particles may form within a mixed cloud (composed of both ice crystals and liquid water drops).</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#BillowCloud -->

    <owl:Class rdf:about="#BillowCloud">
        <rdfs:subClassOf rdf:resource="#Cloud"/>
        <rdfs:comment xml:lang="en"
            >A cloud variety composed of merged or separate elements that are elongated and parallel, either suggestive of ocean waves or arranged in ranks and files.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#BlackShearedAnvil -->

    <owl:Class rdf:about="#BlackShearedAnvil">
        <rdfs:subClassOf rdf:resource="#Cumulonimbus"/>
        <rdfs:comment xml:lang="en"
            >Colloquial expression for a cumulonimbus anvil that spreads upwind into relatively strong winds aloft.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#CentralDenseOvercast -->

    <owl:Class rdf:about="#CentralDenseOvercast">
        <rdfs:subClassOf rdf:resource="#Cumulonimbus"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&atmo;#hasMeteorologicalPhenomena"/>
                <owl:allValuesFrom rdf:resource="&press;#Cyclone"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >The region of dense cloud near the core of a tropical cyclone.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#Cirrostratus -->

    <owl:Class rdf:about="#Cirrostratus">
        <rdfs:subClassOf rdf:resource="#Stratiform"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#Cirrus -->

    <owl:Class rdf:about="#Cirrus">
        <rdfs:subClassOf rdf:resource="#Cloud"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#ClearSky -->

    <owl:Class rdf:about="#ClearSky">
        <rdfs:subClassOf rdf:resource="&atmo;#MeteorologicalPhenomena"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#Cloud -->

    <owl:Class rdf:about="#Cloud">
        <rdfs:subClassOf rdf:resource="&atmo;#MeteorologicalPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&chem;#hasSubstance"/>
                <owl:allValuesFrom rdf:resource="&hydr;#Ice"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&chem;#hasSubstance"/>
                <owl:allValuesFrom rdf:resource="&comp;#CloudCondensationNuclei"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >A visible aggregate of minute water droplets and/or ice particles in the atmosphere above the earth&#39;s surface.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#CloudBase -->

    <owl:Class rdf:about="#CloudBase">
        <rdfs:subClassOf rdf:resource="&planet;#PlanetaryBoundary"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#CloudCover -->

    <owl:Class rdf:about="#CloudCover">
        <rdfs:subClassOf rdf:resource="&atmo;#MeteorologicalPhenomena"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#CloudForcing -->

    <owl:Class rdf:about="#CloudForcing">
        <rdfs:subClassOf rdf:resource="&rad;#RadiativeForcing"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#CloudTop -->

    <owl:Class rdf:about="#CloudTop">
        <rdfs:subClassOf rdf:resource="&planet;#PlanetaryBoundary"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#CondensationLevel -->

    <owl:Class rdf:about="#CondensationLevel">
        <rdfs:label
            >Condensation Level</rdfs:label>
        <rdfs:subClassOf rdf:resource="&atmo;#MeteorologicalPhenomena"/>
        <rdfs:subClassOf rdf:resource="&space;#Level"/>
        <rdfs:comment
            >Level at which water vapor to condense</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#ConvectiveCloudBase -->

    <owl:Class rdf:about="#ConvectiveCloudBase">
        <rdfs:subClassOf rdf:resource="&planet;#PlanetaryBoundary"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#ConvectiveCloudTop -->

    <owl:Class rdf:about="#ConvectiveCloudTop">
        <rdfs:subClassOf rdf:resource="&planet;#PlanetaryBoundary"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#ConvectiveCondensationLevel -->

    <owl:Class rdf:about="#ConvectiveCondensationLevel">
        <rdfs:subClassOf rdf:resource="#CondensationLevel"/>
        <rdfs:comment xml:lang="en"
            >On a thermodynamic diagram, the point of intersection of a sounding curve (representing the vertical distribution of temperature in an atmospheric column) with the saturation mixing ratio line corresponding to the average mixing ratio in the surface layer.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#Cumulonimbus -->

    <owl:Class rdf:about="#Cumulonimbus">
        <rdfs:subClassOf rdf:resource="#Cloud"/>
        <rdfs:comment
            >A principal cloud type (cloud genus), exceptionally dense and vertically developed, occurring either as isolated clouds or as a line or wall of clouds with separated upper portions.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#Cumulus -->

    <owl:Class rdf:about="#Cumulus">
        <rdfs:label>Cumulus</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Cloud"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess"/>
                <owl:allValuesFrom rdf:resource="&trans;#Convection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >A principal cloud type (cloud genus) in the form of individual, detached elements that are generally dense and posses sharp nonfibrous outlines.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#DendriticCrystal -->

    <owl:Class rdf:about="#DendriticCrystal">
        <rdfs:subClassOf rdf:resource="&hydr;#Ice"/>
        <rdfs:comment xml:lang="en"
            >A crystal, particularly a planar ice crystal, with its macroscopic form (crystal habit) characterized by intricate branching structures of a treelike nature.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#EstimatedCeiling -->

    <owl:Class rdf:about="#EstimatedCeiling">
        <rdfs:subClassOf rdf:resource="&space;#Ceiling"/>
        <rdfs:comment xml:lang="en"
            >After U.S.weather observing practice, the ceiling classification applied to a ceiling height that is determined in any of the following ways: 1) by means of a convective-cloud height diagram or dewpoint formula; 2) from the known heights of unobscured portions of natural landmarks, or objects more than one and one-half nautical miles from any runway of the airport; 3) on the basis of observational experience, provided the sky is not obscured by surface-based hydrometeors or lithometeors, and other guides are lacking or considered unreliable; or 4) determined by ceilometer or ceiling light when the penetration of the light beam is in excess of normal for the particular height and type of layer, or when the elevation angle of the clinometer or ceilometer-detector scanner exceeds 84?.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#FlankingLine -->

    <owl:Class rdf:about="#FlankingLine">
        <rdfs:subClassOf rdf:resource="&atmo;#MeteorologicalPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&atmo;#hasMeteorologicalPhenomena"/>
                <owl:allValuesFrom rdf:resource="#Cumulonimbus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&atmo;#hasMeteorologicalPhenomena"/>
                <owl:allValuesFrom rdf:resource="#Cumulus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >An organized lifting zone of cumulus and towering cumulus clouds, connected to and extending outward from the mature updraft tower of a supercell or strong multicell convective storm.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#FoehnCloud -->

    <owl:Class rdf:about="#FoehnCloud">
        <owl:equivalentClass rdf:resource="#FoehnWall"/>
        <owl:equivalentClass rdf:resource="#Moazagotl"/>
        <rdfs:subClassOf rdf:resource="#Cloud"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalPhenomena"/>
                <owl:allValuesFrom rdf:resource="&circ;#Foehn"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >Any cloudform associated with the foehn, usually referring to standing clouds of two types, orographic clouds and mountain wave clouds. Orographic clouds may include crest clouds and the foehn wall. Wave clouds may consist of lenticular (including altocumulus standing lenticular, or ACSL) clouds, lee-wave clouds and cloud bands, and rotors. See also Bishop wave, chinook arch, contessa di vento, Moazagotl.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#FoehnWall -->

    <owl:Class rdf:about="#FoehnWall">
        <rdfs:comment xml:lang="en"
            >The leeward edge of the orographic stratiform cap cloud as seen from the lee side of a mountain barrier, preceding or during a foehn or chinook event. The edge is generally abrupt and resembles a wall of cloud (?foehnwand? in German). This cloud often signifies the occurrence of orographic precipitation, especially snowfall in the cold season, over the peaks. See foehn cloud.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#ForcedCloud -->

    <owl:Class rdf:about="#ForcedCloud">
        <rdfs:subClassOf rdf:resource="#Cumulus"/>
        <rdfs:comment
            >Small cumulus clouds that are everywhere negatively buoyant compared to the surrounding environment, but that exist because the inertia of the rising thermals feeding them from underneath is sufficient to penetrate the lifting condensation level (LCL). Morphologically, these are often cumulus humilis clouds and are typically found at the top of the convective boundary layer during daytime over land, when a strong temperature inversion aloft prevents the clouds from growing deeper. Compare active cloud, passive cloud.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#FunnelCloud -->

    <owl:Class rdf:about="#FunnelCloud">
        <rdfs:subClassOf rdf:resource="#Cloud"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess"/>
                <owl:allValuesFrom rdf:resource="&dynam;#Rotation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >A condensation cloud, typically funnel-shaped and extending outward from a cumuliform cloud, associated with a rotating column of air (a vortex) that may or may not be in contact with the ground. If the rotation is violent and in contact with the ground, the vortex is a tornado. Funnel clouds can occur through a variety of processes in association with convection. For example, small funnel clouds are infrequently seen extending from small, dissipating cumulus clouds in environments with significant vertical wind shear in the cloud-bearing layer.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#KelvinHelmholtzBillows -->

    <owl:Class rdf:about="#KelvinHelmholtzBillows">
        <rdfs:subClassOf rdf:resource="#Cloud"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProperty"/>
                <owl:allValuesFrom rdf:resource="&fluid;#KelvinHelmholtzInstability"/>
