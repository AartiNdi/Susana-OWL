        <rdfs:comment xml:lang="en"
            >A long strip of cloud that sometimes lies against the southern base of Mount Etna in Sicily.  It is said to herald rain.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoCloud.owl#LayerCloud -->

    <owl:Class rdf:about="#LayerCloud">
        <rdfs:subClassOf rdf:resource="#Cloud"/>
