        <owl:versionInfo>2.0</owl:versionInfo>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/astroPlanet.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/atmo.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/atmoBoundary.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/atmoComposition.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/atmoPressure.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/atmoWindMesoscale.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/chem.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/geogLandform.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/hydro.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/phys.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/physDynamics.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/physFluidDynamics.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/physRadiation.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/physSolid.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/physTransport.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/space.owl"/>
    </owl:Ontology>
    


    <!-- 
