     xmlns:phys2="&phys;#"
     xmlns:chem="http://sweet.jpl.nasa.gov/2.0/chem.owl"
     xmlns:atmoPressure="&press;#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:hydr="http://sweet.jpl.nasa.gov/2.0/hydro.owl"
     xmlns:dynam="http://sweet.jpl.nasa.gov/2.0/physDynamics.owl"
     xmlns:chem2="&chem;#"
     xmlns:physDynamics="&dynam;#"
     xmlns:physSolid="&solid;#"
     xmlns:space2="&space;#"
     xmlns:comp="http://sweet.jpl.nasa.gov/2.0/atmoComposition.owl"
     xmlns:geogLandform="&land;#"
     xmlns:physTransport="&trans;#"
     xmlns:atmoBoundary="&bound;#"
     xmlns:atmoComposition="&comp;#"
     xmlns:phys="http://sweet.jpl.nasa.gov/2.0/phys.owl"
     xmlns:solid="http://sweet.jpl.nasa.gov/2.0/physSolid.owl"
     xmlns:trans="http://sweet.jpl.nasa.gov/2.0/physTransport.owl"
     xmlns:circ="http://sweet.jpl.nasa.gov/2.0/atmoWindMesoscale.owl"
     xmlns:physRadiation="&rad;#"
