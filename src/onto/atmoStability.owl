<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY atmo "http://sweet.jpl.nasa.gov/2.0/atmo.owl" >
    <!ENTITY phys "http://sweet.jpl.nasa.gov/2.0/phys.owl" >
    <!ENTITY space "http://sweet.jpl.nasa.gov/2.0/space.owl" >
    <!ENTITY phys2 "http://sweet.jpl.nasa.gov/2.0/phys.owl#" >
    <!ENTITY atmo2 "http://sweet.jpl.nasa.gov/2.0/atmo.owl#" >
    <!ENTITY space2 "http://sweet.jpl.nasa.gov/2.0/space.owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY static "http://sweet.jpl.nasa.gov/2.0/physFluid.owl" >
    <!ENTITY front "http://sweet.jpl.nasa.gov/2.0/atmoFront.owl" >
    <!ENTITY system "http://sweet.jpl.nasa.gov/2.0/sciSystem.owl" >
    <!ENTITY physFluid "http://sweet.jpl.nasa.gov/2.0/physFluid.owl#" >
    <!ENTITY atmoFront "http://sweet.jpl.nasa.gov/2.0/atmoFront.owl#" >
    <!ENTITY thermo "http://sweet.jpl.nasa.gov/2.0/physThermo.owl" >
    <!ENTITY sciSystem "http://sweet.jpl.nasa.gov/2.0/sciSystem.owl#" >
    <!ENTITY proc "http://sweet.jpl.nasa.gov/2.0/chemProcess.owl" >
    <!ENTITY physThermo "http://sweet.jpl.nasa.gov/2.0/physThermo.owl#" >
    <!ENTITY planet "http://sweet.jpl.nasa.gov/2.0/astroPlanet.owl" >
    <!ENTITY obj "http://sweet.jpl.nasa.gov/2.0/spaceObject.owl" >
    <!ENTITY func "http://sweet.jpl.nasa.gov/2.0/mathFunction.owl" >
    <!ENTITY astroPlanet "http://sweet.jpl.nasa.gov/2.0/astroPlanet.owl#" >
    <!ENTITY land "http://sweet.jpl.nasa.gov/2.0/geogLandform.owl" >
    <!ENTITY chemProcess "http://sweet.jpl.nasa.gov/2.0/chemProcess.owl#" >
    <!ENTITY spaceObject "http://sweet.jpl.nasa.gov/2.0/spaceObject.owl#" >
    <!ENTITY calc "http://sweet.jpl.nasa.gov/2.0/mathCalculus.owl" >
    <!ENTITY mathCalculus "http://sweet.jpl.nasa.gov/2.0/mathCalculus.owl#" >
    <!ENTITY mathFunction "http://sweet.jpl.nasa.gov/2.0/mathFunction.owl#" >
    <!ENTITY astab "http://sweet.jpl.nasa.gov/2.0/atmoStability.owl" >
    <!ENTITY geogLandform "http://sweet.jpl.nasa.gov/2.0/geogLandform.owl#" >
    <!ENTITY atmoStability "http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#" >
]>


<rdf:RDF xmlns="&astab;#"
     xml:base="http://sweet.jpl.nasa.gov/2.0/atmoStability.owl"
     xmlns:atmo2="&atmo;#"
     xmlns:atmo="http://sweet.jpl.nasa.gov/2.0/atmo.owl"
     xmlns:physFluid="&static;#"
     xmlns:land="http://sweet.jpl.nasa.gov/2.0/geogLandform.owl"
     xmlns:calc="http://sweet.jpl.nasa.gov/2.0/mathCalculus.owl"
     xmlns:space="http://sweet.jpl.nasa.gov/2.0/space.owl"
     xmlns:atmoFront="&front;#"
     xmlns:physThermo="&thermo;#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:planet="http://sweet.jpl.nasa.gov/2.0/astroPlanet.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:phys2="&phys;#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:func="http://sweet.jpl.nasa.gov/2.0/mathFunction.owl"
     xmlns:atmoStability="&astab;#"
     xmlns:proc="http://sweet.jpl.nasa.gov/2.0/chemProcess.owl"
     xmlns:geogLandform="&land;#"
     xmlns:space2="&space;#"
     xmlns:sciSystem="&system;#"
     xmlns:static="http://sweet.jpl.nasa.gov/2.0/physFluid.owl"
     xmlns:front="http://sweet.jpl.nasa.gov/2.0/atmoFront.owl"
     xmlns:phys="http://sweet.jpl.nasa.gov/2.0/phys.owl"
     xmlns:chemProcess="&proc;#"
     xmlns:spaceObject="&obj;#"
     xmlns:thermo="http://sweet.jpl.nasa.gov/2.0/physThermo.owl"
     xmlns:mathFunction="&func;#"
     xmlns:system="http://sweet.jpl.nasa.gov/2.0/sciSystem.owl"
     xmlns:astab="http://sweet.jpl.nasa.gov/2.0/atmoStability.owl"
     xmlns:mathCalculus="&calc;#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:obj="http://sweet.jpl.nasa.gov/2.0/spaceObject.owl"
     xmlns:astroPlanet="&planet;#">
    <owl:Ontology rdf:about="">
        <owl:versionInfo>2.0</owl:versionInfo>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/astroPlanet.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/atmo.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/atmoFront.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/chemProcess.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/geogLandform.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/mathCalculus.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/mathFunction.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/phys.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/physFluid.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/physThermo.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/sciSystem.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/space.owl"/>
        <owl:imports rdf:resource="http://sweet.jpl.nasa.gov/2.0/spaceObject.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmo.owl#hasMeteorologicalPhenomena -->

    <owl:ObjectProperty rdf:about="&atmo;#hasMeteorologicalPhenomena"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#dTdh -->

    <owl:ObjectProperty rdf:about="#dTdh">
        <rdfs:range rdf:resource="#TemperatureGradient"/>
    </owl:ObjectProperty>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#dTds -->

    <owl:ObjectProperty rdf:about="#dTds">
        <rdfs:range rdf:resource="#TemperatureGradient"/>
    </owl:ObjectProperty>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/chemProcess.owl#hasChemicalProcess -->

    <owl:ObjectProperty rdf:about="&proc;#hasChemicalProcess"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/mathCalculus.owl#derivativeWithRespectTo -->

    <owl:ObjectProperty rdf:about="&calc;#derivativeWithRespectTo"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/mathCalculus.owl#isDerivativeOf -->

    <owl:ObjectProperty rdf:about="&calc;#isDerivativeOf"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/phys.owl#hasPhysicalProcess -->

    <owl:ObjectProperty rdf:about="&phys;#hasPhysicalProcess"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/phys.owl#hasPhysicalProperty -->

    <owl:ObjectProperty rdf:about="&phys;#hasPhysicalProperty"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/sciSystem.owl#approximates -->

    <owl:ObjectProperty rdf:about="&system;#approximates"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://sweet.jpl.nasa.gov/2.0/astroPlanet.owl#Atmosphere -->

    <owl:Class rdf:about="&planet;#Atmosphere"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmo.owl#AtmosphereLayer -->

    <owl:Class rdf:about="&atmo;#AtmosphereLayer"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmo.owl#MeteorologicalPhenomena -->

    <owl:Class rdf:about="&atmo;#MeteorologicalPhenomena"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoFront.owl#Front -->

    <owl:Class rdf:about="&front;#Front"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#AbsoluteInstability -->

    <owl:Class rdf:about="#AbsoluteInstability">
        <rdfs:label
            >Absolute Instability</rdfs:label>
        <rdfs:subClassOf rdf:resource="&system;#Instability"/>
        <rdfs:comment
            >state of a layer of atmosphere when it has superadiabatic lapse rate of temperature</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#AdiabaticAtmosphere -->

    <owl:Class rdf:about="#AdiabaticAtmosphere">
        <rdfs:label
            >Adiabatic Atmosphere</rdfs:label>
        <rdfs:subClassOf rdf:resource="&planet;#Atmosphere"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProperty"/>
                <owl:allValuesFrom rdf:resource="#DryAdiabaticLapseRate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >A model atmosphere characterized by a dry-adiabatic lapse rate throughout its vertical extent.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#AutoConvectiveLapseRate -->

    <owl:Class rdf:about="#AutoConvectiveLapseRate">
        <rdfs:subClassOf rdf:resource="#EnvironmentalLapseRate"/>
        <rdfs:comment xml:lang="en"
            >The environmental lapse rate of temperature in an atmosphere in which the density is constant with height (homogeneous atmosphere), equal to g/R, where g is the acceleration of gravity and R the gas constant.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#BoydenIndex -->

    <owl:Class rdf:about="#BoydenIndex">
        <rdfs:subClassOf rdf:resource="&system;#Stability"/>
        <rdfs:comment
            >A measure of the mean thermodynamic stability in a layer beneath 700 mb.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#Cap -->

    <owl:Class rdf:about="#Cap">
        <rdfs:subClassOf rdf:resource="&space;#Level"/>
        <rdfs:comment
            >A region of negative buoyancy below an existing level of free convection (LFC) where energy must be supplied to the parcel to maintain its ascent.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#ConditionalInstability -->

    <owl:Class rdf:about="#ConditionalInstability">
        <rdfs:subClassOf rdf:resource="&system;#Instability"/>
        <rdfs:comment xml:lang="en"
            >The state of a layer of unsaturated air when its lapse rate of temperature is less than the dry-adiabatic lapse rate but greater than the moist-adiabatic lapse rate.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#ConvectiveInhibition -->

    <owl:Class rdf:about="#ConvectiveInhibition">
        <rdfs:subClassOf rdf:resource="&phys;#Energy"/>
        <rdfs:comment xml:lang="en"
            >The energy needed to lift an air parcel vertically and pseudoadiabatically from its originating level to its level of free convection (LFC).</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#DryAdiabat -->

    <owl:Class rdf:about="#DryAdiabat">
        <rdfs:subClassOf rdf:resource="&func;#Isopleth"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProperty"/>
                <owl:allValuesFrom rdf:resource="#PotentialTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >A line of constant potential temperature on a thermodynamic diagram.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#DryAdiabaticLapseRate -->

    <owl:Class rdf:about="#DryAdiabaticLapseRate">
        <rdfs:subClassOf rdf:resource="#LapseRate"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess"/>
                <owl:allValuesFrom rdf:resource="#DryAdiabaticProcess"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >A process lapse rate of temperature, the rate of decrease of temperature with height of a parcel of dry air lifted by a reversible adiabatic process through an atmosphere in hydrostatic equilibrium.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#DryAdiabaticProcess -->

    <owl:Class rdf:about="#DryAdiabaticProcess">
        <rdfs:label
            >Dry Adiabatic Process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&thermo;#Adiabatic"/>
        <rdfs:comment
            >1. An adiabatic process in a hypothetical atmosphere in which no moisture is present. 2. An adiabatic process in which no condensation of its water vapor occurs and no liquid water is present.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#EnvironmentalLapseRate -->

    <owl:Class rdf:about="#EnvironmentalLapseRate">
        <rdfs:subClassOf rdf:resource="#LapseRate"/>
        <rdfs:comment xml:lang="en"
            >The rate of decrease of temperature with elevation</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#EquivalentPotentialTemperature -->

    <owl:Class rdf:about="#EquivalentPotentialTemperature">
        <rdfs:subClassOf rdf:resource="#PotentialTemperature"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#EvaporativeAvailablePotentialEnergy -->

    <owl:Class rdf:about="#EvaporativeAvailablePotentialEnergy">
        <rdfs:subClassOf rdf:resource="&static;#AvailablePotentialEnergy"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&proc;#hasChemicalProcess"/>
                <owl:allValuesFrom rdf:resource="&proc;#Evaporation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >Analogous to convective available potential energy, except that it is related to the negative buoyancy associated with evaporative cooling of liquid water within a sinking cloudy air parcel.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#FreeConvectiveLayer -->

    <owl:Class rdf:about="#FreeConvectiveLayer">
        <rdfs:subClassOf rdf:resource="&atmo;#AtmosphereLayer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&atmo;#hasMeteorologicalPhenomena"/>
                <owl:allValuesFrom rdf:resource="#FreeInstability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#FreeInstability -->

    <owl:Class rdf:about="#FreeInstability">
        <rdfs:subClassOf rdf:resource="&system;#Instability"/>
        <rdfs:comment xml:lang="en"
            >The state of a layer of unsaturated air when its lapse rate of temperature is less than the dry-adiabatic lapse rate but greater than the moist-adiabatic lapse rate.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#FrontalInversion -->

    <owl:Class rdf:about="#FrontalInversion">
        <rdfs:subClassOf rdf:resource="#Inversion"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&atmo;#hasMeteorologicalPhenomena"/>
                <owl:allValuesFrom rdf:resource="&front;#Front"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >A temperature inversion (temperature rising with height) in the atmosphere, encountered upon vertical ascent through a sloping front (or frontal zone).</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#Inversion -->

    <owl:Class rdf:about="#Inversion">
        <rdfs:subClassOf rdf:resource="&atmo;#MeteorologicalPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProperty"/>
                <owl:allValuesFrom rdf:resource="&thermo;#Temperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >In meteorology, a departure from the usual decrease or increase with altitude of the value of an atmospheric quantity; also, the layer through which this departure occurs (the inversion layer), or the lowest altitude at which the departure is found (the base of the inversion).</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#InversionLayer -->

    <owl:Class rdf:about="#InversionLayer">
        <owl:equivalentClass rdf:resource="#TemperatureInversionLayer"/>
        <rdfs:subClassOf rdf:resource="&atmo;#AtmosphereLayer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&atmo;#hasMeteorologicalPhenomena"/>
                <owl:allValuesFrom rdf:resource="#Inversion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#LFC -->

    <owl:Class rdf:about="#LFC">
        <rdfs:label>LFC</rdfs:label>
        <rdfs:subClassOf rdf:resource="&space;#Level"/>
        <rdfs:comment
            >Level of free convection</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#LapseRate -->

    <owl:Class rdf:about="#LapseRate">
        <rdfs:subClassOf rdf:resource="&system;#Stability"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#Lifting -->

    <owl:Class rdf:about="#Lifting">
        <rdfs:subClassOf rdf:resource="&atmo;#MeteorologicalPhenomena"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#OrographicLifting -->

    <owl:Class rdf:about="#OrographicLifting">
        <rdfs:subClassOf rdf:resource="#Lifting"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess"/>
                <owl:someValuesFrom rdf:resource="&land;#Orographic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >Ascending air flow caused by mountains.  Mechanisms that produce the lifting fall into two broad categories: 1) the upward deflection of horizontal larger-scale flow by the orography acting as an obstacle or barrier; or 2) the daytime heating of mountain surfaces to produce anabatic flow along the slopes and updrafts in the vicinity of the peaks.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#PotentialTemperature -->

    <owl:Class rdf:about="#PotentialTemperature">
        <rdfs:subClassOf rdf:resource="&thermo;#Temperature"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess"/>
                <owl:allValuesFrom rdf:resource="&thermo;#Adiabatic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >The temperature that a parcel would have if brought adiabatically and reversibly from its initial state to a standard reference pressure, typically 100 kPa.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#PseudoAdiabaticProcess -->

    <owl:Class rdf:about="#PseudoAdiabaticProcess">
        <rdfs:subClassOf rdf:resource="&phys;#PhysicalProcess"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&system;#approximates"/>
                <owl:allValuesFrom rdf:resource="&thermo;#Adiabatic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >A moist-adiabatic process in which the liquid water that condenses is assumed to be removed as soon as it is formed, by idealized instantaneous precipitation.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#PseudoEquivalentPotentialTemperature -->

    <owl:Class rdf:about="#PseudoEquivalentPotentialTemperature">
        <rdfs:subClassOf rdf:resource="&thermo;#Temperature"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&system;#approximates"/>
                <owl:allValuesFrom rdf:resource="#EquivalentPotentialTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#TemperatureDepthDerviative -->

    <owl:Class rdf:about="#TemperatureDepthDerviative">
        <rdfs:subClassOf rdf:resource="#TemperatureGradient"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&calc;#derivativeWithRespectTo"/>
                <owl:allValuesFrom rdf:resource="&space;#Depth"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >The derivative of temperature with respect to depth.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#TemperatureGradient -->

    <owl:Class rdf:about="#TemperatureGradient">
        <rdfs:subClassOf rdf:resource="&calc;#Derivative"/>
        <rdfs:subClassOf rdf:resource="&phys;#PhysicalProperty"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&calc;#isDerivativeOf"/>
                <owl:allValuesFrom rdf:resource="&thermo;#Temperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >The derivative of temperature.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#TemperatureHeightDerviative -->

    <owl:Class rdf:about="#TemperatureHeightDerviative">
        <rdfs:subClassOf rdf:resource="#TemperatureGradient"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&calc;#derivativeWithRespectTo"/>
                <owl:allValuesFrom rdf:resource="&space;#Height"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >The derivative of temperature with respect to height.</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#TemperatureInversionLayer -->

    <owl:Class rdf:about="#TemperatureInversionLayer"/>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#ThermalProfile -->

    <owl:Class rdf:about="#ThermalProfile">
        <rdfs:subClassOf rdf:resource="&obj;#Profile"/>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/atmoStability.owl#VirtualPotentialTemperature -->

    <owl:Class rdf:about="#VirtualPotentialTemperature">
        <rdfs:subClassOf rdf:resource="#PotentialTemperature"/>
