    <owl:Class rdf:about="#Ion">
        <rdfs:subClassOf rdf:resource="&chem;#Substance"/>
        <rdfs:comment xml:lang="en"
            >An ion is an atom or molecule where the total number of electrons is not equal to the total number of protons, giving it a net positive or negative electrical charge. [Wikipedia]</rdfs:comment>
    </owl:Class>
    


    <!-- http://sweet.jpl.nasa.gov/2.0/chemIon.owl#Ionization -->

